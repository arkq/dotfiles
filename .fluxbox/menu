[begin] (Fluxbox)
[encoding] {UTF-8}
	[exec] (Run...) {fbrun}
	[exec] (Terminal) {urxvtc}
	[separator]
	[submenu] (Settings) {}
		[config] (Config)
		[workspaces] (Workspace)
		[submenu] (System-Styles) {Styles}
			[stylesdir] (/usr/share/fluxbox/styles)
		[end]
		[submenu] (User-Styles) {Styles}
			[stylesdir] (~/.fluxbox/styles)
		[end]
		[restart] (Restart) {}
	[end]
	[exec] (Lock Screen) {alock -b shade -c glyph -i frame}
	[exit] (Logout)
[endencoding]
[end]
